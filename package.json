{
  "name": "ypm1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "build:css": "npm run scss && npm run autoprefixer",
    "start:app": "browser-sync start --server --files 'dist/css/*.css, dist/js/*.js'",
    "watch:css": "onchange \"src/scss/*.scss\" -- npm run build:css",
    "watch:all": "npm-run-all --parallel start:app watch:css"
  },
  "author": "web-tree@yandex.ru",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "browser-sync": "^2.26.13",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.1.6",
    "postcss-cli": "^8.2.0"
  },
  "dependencies": {}
}
